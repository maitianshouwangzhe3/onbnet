{
    "version": "0.2.0",
    "configurations": [
        {
			"type": "lua",
			"request": "launch",
			"tag": "normal",
			"name": "LuaPanda",
			"cwd": "${workspaceFolder}",
			"luaFileExtension": "",
			"connectionPort": 8818,
			"stopOnEntry": true,
			"useCHook": true,
			"autoPathMode": true,
		},
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/onbnet",
            "args": ["./config"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "debug_onbnet"
        }
    ]
}